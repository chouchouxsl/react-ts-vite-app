{
  "name": "react-ts-vite-app",
  "version": "0.0.0",
  "description": "a Vite2 + Typescript + React + arcoDesign + Less + Eslint + Prettier template",
  "author": {
    "name": "R_h_zero",
    "email": "15146018521@163.com",
    "github": "https://github.com/chouchouxsl"
  },
  "keywords": [
    "vite",
    "typescript",
    "react",
    "redux",
    "arcoDesign",
    "less",
    "eslint",
    "prettier"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint:fix": "eslint --cache --ext .js,.jsx,.ts,.tsx --no-error-on-unmatched-pattern --quiet --fix ./src",
    "lint:style": "stylelint  \"src/**/*.(less|css)\" --fix --custom-syntax postcss-less",
    "prepare": "husky install"
  },
  "dependencies": {
    "@arco-design/web-react": "^2.27.1",
    "@types/react-redux": "^7.1.21",
    "@types/react-router-dom": "^5.3.2",
    "@types/redux": "^3.6.0",
    "axios": "^0.24.0",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "query-string": "^7.0.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "^5.2.1",
    "redux": "^4.1.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/mockjs": "^1.0.4",
    "@types/node": "^17.0.5",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "@vitejs/plugin-react": "^1.0.7",
    "autoprefixer": "^10.4.1",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-typescript": "^0.14.0",
    "husky": "^7.0.4",
    "less": "^4.1.2",
    "lint-staged": "^12.1.5",
    "postcss": "^8.4.5",
    "postcss-less": "^5.0.0",
    "prettier": "^2.5.1",
    "stylelint": "^14.2.0",
    "stylelint-config-css-modules": "^2.3.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-eslint": "^1.3.0",
    "vite-plugin-html": "^2.1.2",
    "vite-plugin-mock": "^2.9.6",
    "vite-plugin-style-import": "^1.4.1"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,json}": [
      "prettier --write",
      "npm run lint:fix",
      "git add"
    ],
    "**/*.{css,less}": [
      "prettier --write",
      "npm run lint:style",
      "git add"
    ],
    "*.{json,md,yml,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "ie >= 10",
    "iOS >= 8",
    "Android >= 4"
  ],
  "license": "MIT"
}
